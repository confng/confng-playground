{
  "app": {
    "name": "Multi-Source ConfNG Example",
    "version": "2.0.0",
    "environment": "development",
    "timeout": 60000,
    "description": "Example demonstrating multiple configuration sources"
  },
  "database": {
    "url": "jdbc:postgresql://localhost:5432/devdb",
    "username": "dev_user",
    "pool": {
      "maxSize": 15,
      "minSize": 3,
      "timeout": 30000
    }
  },
  "features": {
    "newUI": true,
    "analytics": false,
    "caching": true,
    "monitoring": true
  },
  "webdriver": {
    "browser": "firefox",
    "headless": true,
    "timeout": 45
  },
  "api": {
    "baseUrl": "https://api.example.com",
    "timeout": 15000,
    "retryAttempts": 5
  }
}